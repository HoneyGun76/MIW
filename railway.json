{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks",
    "buildCommand": "composer install --no-dev --optimize-autoloader && chmod +x start.sh"
  },
  "deploy": {
    "startCommand": "./start.sh",
    "healthcheckPath": "/health.php",
    "healthcheckTimeout": 100
  }
}
